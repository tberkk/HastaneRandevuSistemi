@model List<Poliklinik>


<h4>@ViewBag.data</h4>
<table class="table table-striped">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Poliklinik Adı</th>
        </tr>
    </thead>
    <tbody>
        @{
            var i = 1;
        }
        @foreach (var x in Model)
        {
            <tr>
                <th>@i</th>
                @{
                    i++;
                }
                <td>@x.PoliklinikAd</td>
                <td><a href="/Home/DoktorGetir/@x.PoliklinikID" class="btn btn-primary">Doktorlar</a></td>
            </tr>
        }
    </tbody>
</table>